    <esysdevAltLibDir>lib\$(esysdevAltOutDirName)</esysdevAltLibDir>
    <boostLibDir>lib\$(boostOutDirName)</boostLibDir>
    <esysdevExeOutDir>bin\$(esysdevOutDirName)\</esysdevExeOutDir>
    <wxLibDir>lib\$(wxOutDirName)</wxLibDir>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='DebugDLL'">
    <esysdevIntRootDir>build\$(esysdevOutDirName)\$(ProjectName)\$(Configuration)\</esysdevIntRootDir>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='ReleaseDLL'">
    <esysdevIntRootDir>build\$(esysdevOutDirName)\$(ProjectName)\$(Configuration)</esysdevIntRootDir>
