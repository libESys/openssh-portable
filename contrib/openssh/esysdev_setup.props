    <esysdevIntRootDir>build\$(esysdevOutDirName)\$(ProjectName)\$(Configuration)\</esysdevIntRootDir>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" Condition="'$(Configuration)'=='Release'">
    <esysdevIntRootDir>build\$(esysdevOutDirName)\$(ProjectName)\$(Configuration)\</esysdevIntRootDir>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros">
    <esyscIncSubDir>$(wxToolkitPrefix)$(esysdevSuffix)</esyscIncSubDir>
